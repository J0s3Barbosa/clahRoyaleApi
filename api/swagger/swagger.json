          "type": "Number",
          "example": 0
        }
        ,
        "Defeat": {
          "type": "Number",
          "example": 0
        }
      }
    },
    "GeneralResponse": {
      "type": "object",
      "properties": {
        "success": {
          "type": "number",
          "description": "returns 1 if successful"
        },
        "description": {
          "type": "string",
          "description": "a short comment"
        },
        "required": [
          "success",
          "description"
        ]
      }
    },
