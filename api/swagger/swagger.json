      },
      "delete": {
        "tags": [
          "CLASH"
        ],
        "summary": "delete a clash by ID",
        "description": "delete a clash in the system\n",
        "operationId": "deletePlanet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "description": "delete a clash",
            "required": true,
            "type": "string",
            "in": "path"
