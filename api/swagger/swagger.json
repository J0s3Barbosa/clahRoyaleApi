      "get": {
        "tags": [
          "CLASH"
        ],
        "summary": "gets Clash info by ID",
        "description": "By passing in the appropriate options, you can search for\navailable clash in the system\n",
        "operationId": "getPlanetWithID",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
